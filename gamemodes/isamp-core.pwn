			}
			return 1;
		}
		//===================================================[DYNAMIC HOUSES SYSTEM]=================================================
	 	if(strcmp(cmd, "/actele", true) == 0)
		{
		    if(IsPlayerConnected(playerid))
		    {
				tmp = strtok(cmdtext, idx);
