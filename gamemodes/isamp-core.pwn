		PlayerInfo[playerid][pID],
		targetName,
		targetIP,
		year,
		month,
		day,
		hour,
		minute,
		second,
		3040,
		12,
		1,
		0,
		0,
		0,
		reason,
		pid,
		issuerName
	);
	mysql_function_query(dbHandle, query, false, "", "");

	foreach(new i : Player) {
	    if(i == playerid) {
	        SendFMessage(i, COLOR_ADMINCMD, "Has sido baneado/a por %s, razón: %s", issuerName, reason);
